<Window x:Class="Godsamme.highscores"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Godsamme"
        mc:Ignorable="d"
        Title="Highscores" Height="800" Width="800" WindowStartupLocation="CenterScreen">
    <Grid>

        <Grid Background="Black" Grid.ColumnSpan="3" Margin="0,0,-0.4,0.4">
            <Button x:Name="highscoresBtn" Content="Go back" HorizontalAlignment="Left" VerticalAlignment="Top" Width="244" Margin="270,613,0,0" Height="62" FontFamily="Jokerman" FontSize="18" Click="highscoresBtn_Click"/>
        </Grid>
        <Image Source="/Resources/highscoresmemory.png" VerticalAlignment="Top" Margin="16,86,35.6,0" HorizontalAlignment="Center" RenderTransformOrigin="0.255,0.444"/>
    </Grid>
</Window>
